    "cypress": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "rollup": "^2.36.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.4",
    "sirv-cli": "^1.0.10",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.26",
    "svelte-i18n": "^3.3.0",
